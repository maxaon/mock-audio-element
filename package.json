{
  "name": "mock-audio-element-adv",
  "version": "0.1.0",
  "description": "minimum implementation of HTML5 Audio events",
  "main": "lib",
  "files": [
    "lib"
  ],
  "engines": {
    "npm": "^3.3.9"
  },
  "scripts": {
    "start": "mocha --compilers js:espower-babel/guess --bail --full-trace --watch",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "posttest": "eslint src test",
    "prepublish": "node -e \"if(process.env.TRAVIS_PASSED){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "babel src --out-dir lib",
    "postinstall": "npm run compile"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "musicmetadata": "^2.0.5",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-xo": "^0.18.2",
    "espower-babel": "^4.0.3",
    "ghooks": "^2.0.0",
    "isparta": "^4.0.0",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "nyc": "^11.1.0",
    "power-assert": "^1.4.4",
    "sinon": "^3.2.1"
  },
  "keywords": [
    "audio",
    "tdd",
    "bdd",
    "test"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "contributors": [
    "Peter K <3187207+maxaon@users.noreply.github.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxaon/mock-audio-element-adv.git"
  },
  "bugs": {
    "url": "https://github.com/maxaon/mock-audio-element-adv/issues"
  },
  "homepage": "https://github.com/maxaon/mock-audio-element-adv#readme",
  "eslintConfig": {
    "extends": "eslint-config-xo",
    "rules": {
      "linebreak-style": 0,
      "indent": [
        "error",
        2
      ]
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
